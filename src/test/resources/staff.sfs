`namespace|scala.ioc`
`namespace x|scala.xml`

"say hello" `#=>` println("Hello, World!")

"say hello once" `#=` println("Hello, World!  You'll have to force me to say it again.")

"hello to..." `#=>` println(s"Hello, ${c("who")}!")

"say hello once again" `#=>` `#ref`("say hello once")

"say hello once again, NOW!" `#=>` `#ref!`("say hello once")

"scope" `#=>` "World"

"scoped hello" `#=>` println(s"Hello, ${`#ref`("scope")}!")

"scoped hello2" `#=>` `#let`("who" -> `#ref`("scope"), `#ref`("hello to..."))

