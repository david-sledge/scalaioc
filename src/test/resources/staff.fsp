`namespace|scalaioc`
`namespace x|scalaioc.xml`

"say hello" `#=>` ((out: java.io.StringWriter) =>
    out.write("Hello, World!"))(`#$`("out"))

"say hello once" `#=` ((out: java.io.StringWriter) =>
    out.write("Hello, World!  You'll have to force me to say it again."))(`#$`("out"))

"hello to..." `#=>` s"Hello, ${`#$`("who")}!"

"say hello once again" `#=>` `#ref`("say hello once")

"say hello once again, NOW!" `#=>` `#ref!`("say hello once")

"scope" `#=>` "World"

"scoped hello" `#=>` ((out: java.io.StringWriter) =>
    out.write(s"Hello, ${`#ref`("scope")}!"))(`#$`("out"))

"scoped hello2" `#=>` `#let`("who" -> `#ref`("scope"), `#ref`("hello to..."))

`#resource`("moreWorkers.fsp")

`#embed`("scala/ioc/xml/macroDefs.fsp")
